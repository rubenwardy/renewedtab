{
	"manifest_version": 3,
	"name": "__MSG_appName__",
	"version": "1.20.1",
	"description": "__MSG_appDescription__",
	"author": "rubenwardy",
	"homepage_url": "https://renewedtab.com/",
	"minimum_chrome_version": "100",
	"default_locale": "en",
	"icons": {
		"32": "icon-32.png",
